{
  "name": "gyapak-proctor",
  "version": "1.0.0",
  "main": "main.js",
  "description": "AI Proctoring App for Exams",
  "author": "Your Name or Org",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.gyapak.proctor",
    "productName": "GyapakProctor",
    "files": [
      "**/*",
      "bin/**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "electron": "^35.2.0",
    "electron-builder": "^26.0.12"
  }
}
